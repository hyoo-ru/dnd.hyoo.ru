$hyoo_dungeon_char_main $mol_page
	title \Основное
	char $hyoo_dungeon_char
		name? => name?
		story? => story?
		biography? => biography?
		age? => age?
		level? => level?
		experience? => experience?
		race? => race?
		classes? => classes?
	body /
		<= Grade $mol_view sub /
			<= Level_block $mol_form_field
				name \🏅Уровень
				Content <= Level $mol_paginator
					value? <=> level?
			<= Experience_block $mol_form_field
				name \✨Опыт
				Content <= Experience $mol_number
					value? <=> experience?
					precision_change 100
			<= Age_block $mol_form_field
				name \📅Возраст
				Content <= Age $mol_number
					value? <=> age?
		<= Base_block $mol_form_group sub /
			<= Name_block $mol_form_field
				name \Имя
				Content <= Name $mol_string
					value? <=> name?
			<= Story_block $mol_form_field
				name \Предыстория
				Content <= Story $mol_string
					value? <=> story?
		<= Race_block $mol_form_field
			name \Раса
			Content <= Race $mol_switch
				value? <=> race?
				keys <= race_options /string
				option_title* <= race_title* \
		<= Classes_block $mol_form_field
			name \Классы
			Content <= Classes $mol_check_list
				option_checked*? <=> class_selected*? false
				keys <= class_options /string
				option_title* <= class_title* \
		<= Biography_block $mol_form_field
			name \Биография
			Content <= Biography $mol_textarea
				value? <=> biography?
