$hyoo_dungeon_char_summary $mol_page
	title \Сводка
	char $hyoo_dungeon_char
		name => name
		biography => biography
		age => age
		level? => level?
		experience? => experience?
		moral => moral
		ethics => ethics
		traits => traits
		ideals => ideals
		affection => affection
		weakness => weakness
		master_bonus => master_bonus
		hits? => hits?
		hits_addon? => hits_addon?
		hits_max? => hits_max?
		hits_dice => hits_dice
		hits_heal => hits_heal
	Ability_cofig $hyoo_dungeon_ability_config
		char <= char
		ability_list => ability_list
		Ability_addon null
	Skill_cofig $hyoo_dungeon_skill_config
		char <= char
		skill_list => skill_list
		Skill_addon null
	body /
		<= Columns $mol_view sub /
			<= Info $mol_list rows /
				<= Top $mol_view sub /
					<= Base $mol_view sub /
						<= Name $mol_chip
							hint \Имя
							title <= name
						<= Age $mol_chip
							hint \Возраст
							sub /
								\📅
								<= age
						<= Speed $mol_chip
							hint \Скорость
							sub /
								\🦶
								<= speed 1
				<= Image $mol_image uri <= image \
				<= Main $mol_view sub /
					<= Life $mol_view sub /
						<= Race $mol_link
							hint \Раса
							title <= race_title \
							uri <= race_link \
						<= Class $mol_link
							hint \Класс
							title <= class_title \
							uri <= class_link \
						<= Story $mol_link
							hint \Предыстория
							title <= story_title \
							uri <= story_link \
					<= Alignment $mol_view sub /
						<= Goodness $mol_chip
							hint \Мораль
							title <= moral_title \
						<= Principality $mol_chip
							hint \Этика
							title <= ethics_title \
				<= Personality $mol_list rows /
					<= Traits $mol_text text <= traits
					<= Ideals $mol_text text <= ideals
					<= Affection $mol_text text <= affection
					<= Weakness $mol_text text <= weakness
					<= Biography $mol_text text <= biography
			<= Stat $mol_view sub /
				<= Stat_main $mol_list rows /
					<= Grade_block $mol_list rows /
						<= Level $hyoo_dungeon_parameter
							title \🏅Уровень
							Value <= Level_value $mol_paginator
								value? <=> level?
						<= Master_bonus $hyoo_dungeon_parameter
							title \🥇Мастерство
							value /
								\+
								<= master_bonus
						<= Experience $hyoo_dungeon_parameter
							title \✨Опыт
							Value <= Experience_value $mol_paginator
								value? <=> experience?
								step 100
					<= Hits_block $mol_list rows /
						<= Hits_max $hyoo_dungeon_parameter
							title \💟Предел
							Value <= Hits_max_value $mol_paginator
								value? <=> hits_max?
						<= Hits $hyoo_dungeon_parameter
							title \💗Здоровье
							Value <= Hits_value $mol_paginator
								value? <=> hits?
						<= Hits_addon $hyoo_dungeon_parameter
							title \💖Бафы
							Value <= Hits_addon_value $mol_paginator
								value? <=> hits_addon?
						<= Hits_heal $hyoo_dungeon_parameter
							title \💊Лечение
							value / <= hits_heal
					<= Ability_list $mol_view
						sub <= ability_list
					<= Perks $mol_text
						text <= perks \
				<= Skill_list $mol_view sub <= skill_list
